{
    "name": "xxallanxx/laravel-menus",
    "description": "Laravel Menu management",
    "keywords": [
      "laravel",
      "menus",
      "navigation",
      "nav",
      "bootstrap"
    ],
    "license": "MIT",
    "authors": [
    
      {
        "name": "Allan Henrique",
        "email": "allandesinop@gmail.com"
      },
      {
        "name": "Pingpong Labs",
        "email": "pingpong.labs@gmail.com"
      }
    ],
    "require": {
      "php": ">=8.0",
        "illuminate/support": "^9.50",
        "illuminate/config": "^9.50",
        "illuminate/view": "^9.50",
        "laravelcollective/html": "^6.3"
     
    },
    "require-dev": {
      "phpunit/phpunit": "^9.6",
      "mockery/mockery": "^1.5",
      "orchestra/testbench": "^7.21",
      "friendsofphp/php-cs-fixer": "^3.14"
  },
    "autoload": {
      "psr-4": {
        "xxallanxx\\Menus\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "xxallanxx\\Menus\\Tests\\": "Tests"
      }
    },
    "extra": {
      "branch-alias": {
        "dev-master": "6.0-dev"
      },
      "laravel": {
        "providers": [
          "xxallanxx\\Menus\\MenusServiceProvider"
        ],
        "aliases": {
          "Menu": "xxallanxx\\Menus\\Facades\\Menu"
        }
      }
    },
    "minimum-stability": "stable"
   
}
